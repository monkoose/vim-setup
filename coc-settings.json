{
    "suggest.snippetIndicator": " ~",
    "suggest.maxCompleteItemCount": 30,
    "suggest.timeout": 3000,
    "suggest.highPrioritySourceLimit": 20,
    "suggest.lowPrioritySourceLimit": 10,
    "suggest.removeDuplicateItems": true,
    "diagnostic.enableMessage": "jump",
    "diagnostic.messageDelay": 100,
    "diagnostic.floatConfig": {
        "maxWidth": 100,
        "maxHeight": 10
    },
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": "E",
    "diagnostic.warningSign": "W",
    "diagnostic.infoSign": "I",
    "diagnostic.hintSign": "H",
    "signature.enable": false,
    "signature.floatConfig": {
        "maxHeight": 6
    },
    "hover.floatConfig": {
        "maxWidth": 100
    },
    "notification.highlightGroup": "CocNotification",
    "list.alignColumns": true,
    "list.height": 12,
    "list.limitLines": 20000,
    "list.maxPreviewHeight": 10,
    "list.previewSplitRight": true,
    "list.source.diagnostics.pathFormat": "short",
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.extensionUpdateCheck": "never",
    "coc.preferences.formatOnSaveFiletypes": [],
    "coc.preferences.rootPatterns": [
        ".git"
    ],
    "coc.preferences.willSaveHandlerTimeout": 1000,
    "list.insertMappings": {
        "<C-l>": "do:defaultaction",
        "<C-a>": "do:selectall",
        "<C-p>": "do:previewup",
        "<C-n>": "do:previewdown",
        "<C-o>": "do:previewtoggle",
        "<C-r>": "do:refresh",
        "<C-e>": "do:toggle",
        "<C-q>": "action:quickfix",
        "<C-v>": "action:vsplit",
        "<C-x>": "action:split",
        "<C-t>": "action:tabe"
    },
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
            "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                },
                "clang": {
                    "extraArgs": [ "-Isrc" ]
                }
            }
        }
    },
    "d.servedPath": "/home/monkoose/.config/coc/extensions/coc-dlang-data/serve-d"
}
