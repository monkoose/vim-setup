  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "diagnostic.maxWindowHeight": 10,
  "signature.maxWindowHeight": 6,
  "hover.floatMaxWidth": 100,
  "notification.highlightGroup": "CocNotification",
  "list.alignColumns": true,
  "list.height": 12,
  "list.limitLines": 20000,
