{
    "suggest.autoTrigger": "trigger",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "suggest.labelMaxLength": 60,
    "suggest.minTriggerInputLength": 2,
    "suggest.maxCompleteItemCount": 50,
    "suggest.timeout": 3000,
    "suggest.highPrioritySourceLimit": 30,
    "suggest.lowPrioritySourceLimit": 20,
    "suggest.removeDuplicateItems": true,
    "suggest.defaultSortMethod": "alphabetical",
    "suggest.triggerCompletionWait": 0,
    "suggest.filterGraceful": false,
    "suggest.floatConfig": {
        "border": false,
        "highlight": "CocFloating"
    },
    "outline.splitCommand": "botright 40vs",
    "diagnostic.enableMessage": "jump",
    "diagnostic.autoRefresh": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": "E",
    "diagnostic.enableSign": false,
    "diagnostic.hintSign": "H",
    "diagnostic.infoSign": "I",
    "diagnostic.warningSign": "W",
    "diagnostic.format": "%message",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.virtualTextFormat": "",
    "diagnostic.virtualTextPrefix": " ‚óè",
    "inlayHint.enable": false,
    "signature.enable": false,
    "signature.hideOnTextChange": true,
    "floatFactory.floatConfig": {
        "border": true,
        "highlight": "CocFloating",
        "close": false
    },
    "signature.floatConfig": {
        "maxHeight": 6
    },
    "hover.floatConfig": {
        "maxWidth": 100
    },
    "notification.timeout": 5000,
    "notification.disabledProgressSources": ["*"],
    "list.statusLineSegments": [
        "%#CocListMode#  COC %{coc#list#status('args')}%*",
        "%1(%{coc#list#status('loading')}%)",
        "%L/%{coc#list#status('total')} - %l ",
        "%=",
        "%#CocListPath# %{coc#list#status('cwd')} %*"
    ],
    "list.alignColumns": false,
    "list.floatPreview": true,
    "list.height": 10,
    "list.limitLines": 20000,
    "list.maxPreviewHeight": 8,
    "list.previewHighlightGroup": "Visual",
    "list.source.diagnostics.pathFormat": "short",
    "list.previewToplineOffset": 2,
    "pullDiagnostic.onChange": false,
    "pullDiagnostic.onSave": true,
    "coc.preferences.enableMessageDialog": false,
    "coc.preferences.extensionUpdateCheck": "never",
    "coc.preferences.promptInput": false,
    "coc.preferences.formatOnSave": false,
    "workspace.rootPatterns": [
        ".git"
    ],
    "coc.preferences.willSaveHandlerTimeout": 1000,
    "list.insertMappings": {
        "<C-l>": "do:defaultaction",
        "<C-a>": "do:selectall",
        "<C-p>": "do:previewup",
        "<C-n>": "do:previewdown",
        "<C-o>": "do:previewtoggle",
        "<C-r>": "do:refresh",
        "<C-e>": "do:toggle",
        "<C-q>": "action:quickfix",
        "<C-v>": "action:vsplit",
        "<C-x>": "action:split",
        "<C-t>": "action:tabe"
    },
    "svelte.enable-ts-plugin": true,
    "Lua.runtime.version": "LuaJIT",
    "Lua.completion.callSnippet": "Replace"
}
